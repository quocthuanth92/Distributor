@using CMS.Data
@model List<Navigation>
@if (Model.Count() > 0)
{
    var listParent = Model.Where(l => l.ParentID == 0).ToList();

                foreach (var item in listParent)
                {
                    <a href="/@item.Link">@item.Name</a><span> | </span>
                }
                <a href="/Detail/Privacy_Policy">Privacy Policy</a>
}
