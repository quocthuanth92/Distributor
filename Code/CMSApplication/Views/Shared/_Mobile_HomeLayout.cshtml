<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>@CMS.Common.Helpers.WebHelper.TrimStringForTitle(@ViewBag.Title + @Html.Action("SettingByName", "FESetting", new { SettingName = "title" }))</title>
    <meta name="Keywords" content="@CMS.Common.Helpers.WebHelper.TrimStringForTitle(Html.Action("SettingByName", "FESetting", new { SettingName = "keywords" }).ToString() + " , " + ViewBag.Keywords)" />
    <meta name="Description" content="@CMS.Common.Helpers.WebHelper.TrimStringForTitle(Html.Action("SettingByName", "FESetting", new { SettingName = "Description" }).ToString()+" - "+ViewBag.Description)" />

    <meta name="viewport" content="width=device-width, initial-scale=1">

    <link rel="stylesheet" href="@Url.Content("~/Content/mobile/assets/jquery-mobile/jquery.mobile.structure-1.3.1.css")" />
    <link rel="stylesheet" href="@Url.Content("~/Content/mobile/css/font-awesome.css")" />
    <link rel="stylesheet" href="@Url.Content("~/Content/mobile/assets/flex-slider/flexslider.css")" type="text/css">

    <link rel="stylesheet" href="@Url.Content("~/Content/mobile/css/style.css")" />

    <script src="http://code.jquery.com/jquery-1.8.3.min.js"></script>
    <script src="http://code.jquery.com/mobile/1.3.0/jquery.mobile-1.3.0.min.js"></script>
    <script src="@Url.Content("~/Content/mobile/assets/flex-slider/jquery.flexslider-min.js")"></script>
    <script src="@Url.Content("~/Content/mobile/js/main.js")"></script>
    <script type="text/javascript" src="http://maps.googleapis.com/maps/api/js?key=AIzaSyDq7skUApJ5P6nmEtamguXV6neqigI4JLk&amp;sensor=false">
</script>
     <link href="../../Content/css/fancybox/jquery.fancybox.css" rel="stylesheet" />
    <script type="text/javascript" src="../../Content/js/plugins/fancybox/jquery.fancybox.pack.js"></script>
</head>
<body>
    <div data-role="page" data-theme="a">
        <!-- Side Navigation Panel -->
        <div data-role="panel" id="sideNav" data-position="left">
            <ul id="sideButtons">
                <li>
                    <a href="#"><i class="icon-facebook"></i></a>
                </li>
                <li>
                    <a href="#"><i class="icon-twitter"></i></a>
                </li>
                <li>
                    <a href="#"><i class="icon-linkedin"></i></a>
                </li>
                <li>
                    <a href="#"><i class="icon-google-plus"></i></a>
                </li>
            </ul>
            
            @Html.Action("Index", "Navigation")
        </div>
        <!-- End Side Navigation -->
        @* Begin of Body *@
        @RenderBody()
        @* End of Body *@
    </div>
    <!-- /page -->
</body>
</html>
